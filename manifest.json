{
  "name": "Tab Manager",
  "description": "Manage tabs in the browser",
  "version": "1.2.0",
  "manifest_version": 3,
  "icons": {
    "16": "./images/icon-16.png",
    "32": "./images/icon-32.png",
    "48": "./images/icon-48.png",
    "128": "./images/icon-128.png"
  },
  "action": {
    "default_popup": "./popup/popup.html"
  },
  "permissions": ["tabGroups", "tabs"]
}
